#include <string>
#include <vector>
#include <iostream>

using namespace std;

int main()
{
    int num1 = 0;
    int num2 = 0;
    int answer = 0;


    cout << "첫번째 숫자를 입력하시오 : ";
    cin >> num1;

    cout << "두번쨰 숫자를 입력하시오 : ";
    cin >> num2;

    answer = num1 - num2;


    cout << "첫번째 숫자가" << num1 << "이고 두번째 숫자가" << num2 << "이므로" << num1 << "-" << num2 << "=" << answer << "를 return합니다";


        return answer;


}
